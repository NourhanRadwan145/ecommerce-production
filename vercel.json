{
    "version": 2,
    "builds": [
      {
        "src": "Back-end/src/Servers/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/front-end/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Back-end/src/Servers/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/front-end/$1"
      }

    ]
    
  }
  